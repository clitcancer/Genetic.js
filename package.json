{
	"name": "geneticjs",
	"version": "1.0.0",
	"description": "Simple and clear JavaScript class for genetic algorithms. Contains helpful functions to get you going.",
	"main": "./dist/Genetic.js",
	"types": "./dist/Genetic.d.ts",
	"scripts": {
		"test": "jest --no-cache",
		"build": "tsc",
		"prepare": "npm run build && npm run browser",
		"prepublishOnly": "pnpm test",
		"postversion": "git push && git push --tags",
		"browser": "npx browserify ./dist/Genetic.js --standalone genetic | npx uglify-es -o ./dist/Genetic.browser.min.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "pnpm test"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/clitcancer/Genetic.js.git"
	},
	"keywords": [
		"GA",
		"genetic",
		"genetic-algorithm",
		"neuro-evolution"
	],
	"author": "Marcin Wojnarowski @shilangyu",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/clitcancer/Genetic.js/issues"
	},
	"homepage": "https://github.com/clitcancer/Genetic.js#readme",
	"devDependencies": {
		"@types/jest": "^23.3.9",
		"browserify": "^16.2.3",
		"husky": "^1.3.1",
		"jest": "^23.6.0",
		"ts-jest": "^23.10.4",
		"typescript": "^3.1.6",
		"uglify-es": "^3.3.9"
	},
	"files": [
		"dist/**/*"
	]
}
